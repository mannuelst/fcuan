%REGRAS
comunaPro(C, P):-
	comuna(C, M),
	municipio(M, P),
	provincia(P, angola).

comunaPais(C, angola):-
	comuna(C, M),
	municipio(M, P),
	provincia(P, angola).

distritoPro(D, P):-
	distrito(D, M),
	municipio(M, P),
	provincia(P, angola).

distritoPais(D, angola):-
	distrito(D, M),
	municipio(M, P),
	provincia(P, angola).

pais_dist(angola, D):-
	distrito(D, M),
	municipio(M, P),
	provincia(P, angola).

pais_mun(angola, M):-
	municipio(M, P),
	provincia(P, angola).

pais_comun(angola, C):-
	comuna(C, M),
	municipio(M, P),
	provincia(P, angola).
pais_prov(angola, P):-
	provincia(P, angola).

prov_dist(P, D):-
	distrito(D, M),
	municipio(M, P).


prov_mun(P, M):-
	municipio(M, P).

mun_dist(M, D):-
	distrito(D, M).

mun_comun(M, C):-
	comuna(C, M).


distritoMunPro(D, municipio(M, provincia(P))):-
	distrito(D, M),
	municipio(M, P),
	provincia(P, angola).

distritoMunProAng(D, municipio(M, provincia(P, angola))):-
	distrito(D, M),
	municipio(M, P),
	provincia(P, angola).
